@import '_config.pcss';

.uploadcare-dialog-close {
	position: absolute;
	z-index: 1001;
	top: 0;
	right: 0;

	width: var(--dialog-header-height);
	height: var(--dialog-header-height);

	cursor: pointer;
	text-indent: -9999px;

	background-color: red; /* TODO change to close icon */
	background-repeat: no-repeat;
	background-position: 50% 50%;
}
