.progress__canvas {
  width: 100%;
  height: 100%;
}

.progress__text-container {
  width: 100%;
  height: 100%;
  display: table;
  white-space: normal;
}

.progress__text {
  display: table-cell;
  vertical-align: middle;
  text-align: center;
  font-size: 60%;
  line-height: 1;
}

.progress_type_canvas {
  color: #ffd800;
  border-color: #e1e5e7;
}
