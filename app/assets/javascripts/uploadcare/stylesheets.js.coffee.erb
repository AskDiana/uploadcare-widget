# = require ./widget/templates/styles

uploadcare.whenReady ->
  {jQuery: $} = uploadcare
  css = JST['uploadcare/widget/templates/styles']()

  style = document.createElement('style')
  if style.styleSheet?
    style.styleSheet.cssText = css
  else
    style.appendChild(document.createTextNode(css))
  $('head').append(style)