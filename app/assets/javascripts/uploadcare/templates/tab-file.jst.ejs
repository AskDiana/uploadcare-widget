<div class="uploadcare--panel__content uploadcare--draganddrop">
  <div class="uploadcare--text uploadcare--text_extra-large uploadcare--dragging__show">
    <%= t('draghere') %>
  </div>

  <div class="uploadcare--panel__title uploadcare--dragging__hide">
    <div class="uploadcare--draganddrop__supported">
      <div class="uploadcare--text uploadcare--text_extra-large">
        <%= t('dialog.tabs.file.drag') %>
      </div>
      <div class="uploadcare--text uploadcare--text_small uploadcare--text_muted">
        <%- t('dialog.tabs.file.or') %>
      </div>
    </div>

    <div class="uploadcare--text uploadcare--text_large uploadcare--draganddrop__not-supported">
      <%= t('dialog.tabs.file.nodrop') %>
    </div>
  </div>

  <div class="uploadcare--button uploadcare--button_size_big uploadcare--button_primary uploadcare--panel__action-button needsclick  uploadcare--dragging__hide">
    <%- t('dialog.tabs.file.button') %>
  </div>

  <div class="uploadcare--file-sources uploadcare--dragging__hide">
    <div class="uploadcare--text uploadcare--text_small uploadcare--text_muted uploadcare--file-sources__caption">
      <%- t('dialog.tabs.file.also') %>
    </div>
    <div class="uploadcare--file-sources__list"></div>
  </div>
</div>
